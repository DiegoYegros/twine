CREATE TABLE feed(
  name TEXT NOT NULL,
  icon TEXT NOT NULL,
  description TEXT NOT NULL,
  homepageLink TEXT NOT NULL,
  link TEXT NOT NULL PRIMARY KEY
);

CREATE INDEX feed_link_index ON feed(link);

insert:
INSERT OR REPLACE INTO feed(name, icon, description, homepageLink, link)
VALUES ?;

remove:
DELETE FROM feed WHERE link = :link;

feeds:
SELECT * FROM feed;
